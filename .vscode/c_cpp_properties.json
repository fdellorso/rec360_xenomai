            "compilerPath": "${workspaceFolder}/tools/arm-bcm2708/gcc-linaro-arm-linux-gnueabihf-raspbian-x64/bin/arm-linux-gnueabihf-gcc",
            "cStandard": "c89",
            "cppStandard": "c++98",
            "intelliSenseMode": "gcc-arm"
